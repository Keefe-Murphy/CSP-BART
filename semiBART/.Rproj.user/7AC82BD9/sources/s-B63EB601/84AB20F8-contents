library(grf)
library(glmnet)
Y=dados    #The first column contains the dependent variables and the third-123th column contains the x-variables
dum=rep(0,nrow(Y))
dum[which.min(Y[,1])]=1
Y=cbind(Y,dum=dum)
y = Y[,1]

nprev= 132

X = Y[1:(nrow(Y)-nprev),3:123]
y = Y[1:(nrow(Y)-nprev),1]

localLinearForest = ll_regression_forest(X,y)
X_test = Y[360:491,3:123]
predLLForest = predict(localLinearForest, X_test)

